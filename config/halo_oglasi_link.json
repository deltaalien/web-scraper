{
    "url":"https://www.halooglasi.com",
    "pagination_format": "/nekretnine/izdavanje-stanova?page=%d",
    "pagination_type": "ITERATION_STANDARD",
    "stop": "NO_DATA",
    "extraction": "LINK_EXTRACTOR",

    "item" : {
       "query" : "h3.product-title a",
        "data_attribute":"href"
    },
    "data" : {
        "name": {
            "query" : "h1.product-details-title span",
            "data_attribute":""
        },
        "price": {
            "query" : "span.offer-price span.offer-price-value",
            "data_attribute":""
        },
        "description": {
            "query": "div.tab-top-group-attr-value span",
            "data_attribute":""
        },
        "image": {
            "query": "img",
            "data_attribute": "src"
        },
        "date": {
            "query": "strong#plh82",
            "data_attribute": ""
        }
    }
}